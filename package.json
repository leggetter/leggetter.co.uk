{
  "name": "leggetter.co.uk",
  "version": "1.0.0",
  "description": "www.leggetter.co.uk website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "git subtree push --prefix _site origin gh-pages",
    "dev": "bundle exec jekyll serve -w --future -l --incremental",
    "dev:docker": "git submodule init && git submodule sync && git submodule update && npm run generate-family-tree && docker build -t leggetter-jekyll . && docker run --rm -p 4000:4000 -v $(pwd):/site leggetter-jekyll",
    "build": "bundle exec jekyll build",
    "copy-images": "cp -r ./_includes/realtime-web-technologies-guide/images ./_site/real-time-web-technologies-guide/images",
    "prebuild": "git submodule init && git submodule sync && git submodule update",
    "generate-family-tree": "python3 leggetter-family-tree/tree-view-family-chart.py leggetter-family-tree/0001tree.gen --output-jekyll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leggetter/leggetter.co.uk.git"
  },
  "keywords": [
    "website",
    "jekyll",
    "leggetter",
    "real-time",
    "web",
    "technology"
  ],
  "author": "Phil Leggetter <phil@leggetter.co.uk> (https://leggetter.co.uk)",
  "license": "Creative Commons - Attribution 3.0",
  "bugs": {
    "url": "https://github.com/leggetter/leggetter.co.uk/issues"
  },
  "homepage": "https://github.com/leggetter/leggetter.co.uk",
  "resolutions": {
    "graceful-fs": "^4.2.4",
    "gift": "^0.10.2"
  }
}
